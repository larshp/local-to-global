{
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-console": ["off"],
    "no-trailing-spaces": ["error"],
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "no-var": ["error"],
    "brace-style": ["error", "1tbs"]
  }
}
